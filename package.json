{
	"name": "edh-dashboard",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"test": "npm run test:unit && npm run test:e2e",
		"test:unit": "vitest run",
		"test:unit:watch": "vitest",
		"test:e2e": "playwright test",
		"test:e2e:watch": "playwright test --ui",
		"test:watch": "concurrently -k -n unit,e2e -c blue,magenta \"npm run test:unit:watch\" \"npm run test:e2e:watch\"",
		"e2e": "PLAYWRIGHT_SKIP_VALIDATE_HOST_REQUIREMENTS=1 playwright test",
		"lint": "prettier --check . && eslint .",
		"format": "prettier --write .",
		"format:check": "prettier --check .",
		"pre-push-check": "npm run format:check && npm run lint && npm run check && npm run test",
		"prepare": "husky install"
	},
	"devDependencies": {
		"@playwright/test": "^1.46.0",
		"@skeletonlabs/skeleton": "^2.11.0",
		"@skeletonlabs/tw-plugin": "^0.4.1",
		"@sveltejs/adapter-vercel": "^5.0.0",
		"@sveltejs/kit": "^2.0.0",
		"@sveltejs/vite-plugin-svelte": "^3.0.0",
		"@tailwindcss/forms": "^0.5.10",
		"@tailwindcss/typography": "^0.5.16",
		"@testing-library/jest-dom": "^6.6.3",
		"@testing-library/svelte": "^4.2.0",
		"@types/eslint": "^9.6.0",
		"@types/node": "^22.13.10",
		"autoprefixer": "^10.4.21",
		"concurrently": "^9.1.2",
		"eslint": "^9.0.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-svelte": "^2.36.0",
		"globals": "^15.0.0",
		"husky": "^9.1.5",
		"jsdom": "^24.1.3",
		"postcss": "^8.5.3",
		"prettier": "^3.1.1",
		"prettier-plugin-svelte": "^3.1.2",
		"svelte": "^4.2.7",
		"svelte-check": "^4.0.0",
		"tailwindcss": "^3.4.17",
		"typescript": "^5.0.0",
		"typescript-eslint": "^8.0.0",
		"vite": "^5.0.3",
		"vite-plugin-tailwind-purgecss": "^0.3.5",
		"vitest": "^1.6.0"
	},
	"type": "module",
	"dependencies": {
		"@vercel/postgres": "^0.10.0",
		"@upstash/redis": "^1.34.3",
		"googleapis": "^166.0.0",
		"uuid": "^13.0.0"
	}
}
